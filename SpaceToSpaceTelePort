-- 다른 스페이스 아이디
local PlaceID = 4799194775

-- 파트에 터치되면 텔리포팅 기능 가동
script.Parent.Touched:Connect(function(hit)
   -- 플레이어 정보 가져오기
	 local player = game.Players:GetPlayerFromCharacter(hit.parent)
	 
  --플레이어 정보가 확인되었다면 텔리포팅 시작
	if player then
		print(player)
		TelePortService:Teleport(PlaceID, player)
	end
end)
